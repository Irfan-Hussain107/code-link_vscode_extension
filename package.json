{
  "name": "code-link",
  "displayName": "Code Link",
  "description": "Collaborate in real-time on any file. Share a simple room ID to invite others to code with you.",
  "version": "0.1.1",
  "publisher": "IrfanHussain",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Irfan-Hussain107/code-link_vscode_extension.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeLink.startSession",
        "title": "Code Link: Start Collaboration Session"
      },
      {
        "command": "codeLink.joinSession",
        "title": "Code Link: Join Collaboration Session"
      },
      {
        "command": "codeLink.stopSession",
        "title": "Code Link: Stop Collaboration Session"
      }
    ],
    "configuration": {
      "title": "Code Link",
      "properties": {
        "codeLink.displayName": {
          "type": "string",
          "default": "",
          "description": "Your name to display to other collaborators."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "express": "^5.1.0",
    "nanoid": "^3.3.4",
    "y-websocket": "^1.5.3",
    "yjs": "^13.6.8"
  }
}
